{

    "version": 2,
    
    "functions": {
      "api/*": {
        "runtime": "nodejs14.x",
        "maxDuration": 30
      }
    },
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
       
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }     
    ]
      
   

}

  
