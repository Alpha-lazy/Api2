{

    "version": 2,
    
    
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
       
      },
      {"api/*": {
        "runtime": "nodejs14.x",
        "maxDuration": 30
      }}
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }     
    ]
      
   

}

  
